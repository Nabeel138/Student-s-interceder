    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run build:server && npm run docker:build && npm run docker:push",
    "docker:build": "docker build -t registry.gitlab.com/navix/dhub/ngx-kit/alma .",
    "docker:run": "docker run -d -p 4001:4000 registry.gitlab.com/navix/dhub/ngx-kit/alma",
    "docker:push": "docker push registry.gitlab.com/navix/dhub/ngx-kit/alma"
  },
  "private": true,
