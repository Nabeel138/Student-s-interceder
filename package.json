  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:browser": "ng build --prod",
    "build:server": "npm run build:browser && npm run build:server:app && npm run build:server:bundle",
    "build:server:app": "ng run ui-alma:server",
    "build:server:bundle": "webpack --config webpack.server.config.js --progress --colors",
